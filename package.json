  "scripts": {
    "build": "npm run build:prisma && npm run build:next",
    "build:next": "next build",
    "build:prisma": "prisma generate",
    "db:migrate": "npm run db:migrate:core && npm run db:migrate:worker",
    "db:migrate:core": "prisma migrate deploy",
    "db:migrate:worker": "dotenv -- tsx scripts/migrate-worker.ts",
    "db:reset": "psql -h 127.0.0.1 -U postgres -f dev/reset.sql",
    "db:seed": "psql -h 127.0.0.1 -U postgres -f dev/seed.sql",
    "db:setup": "npm run db:reset && npm run db:migrate && npm run db:seed && npm run build:prisma",
    "dev": "TZ=UTC next dev --turbopack",
    "lint": "next lint",
    "start": "TZ=UTC next start",
    "typecheck": "tsc",
    "cli": "tsx src/server/cli.ts"
  },
